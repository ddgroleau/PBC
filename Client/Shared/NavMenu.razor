@inject ILazor e

<nav class="navbar fixed-top navbar-expand-sm navbar-light" style="color:rgba(0,0,0,0) !important"><p>DummyNavBar</p></nav>
<nav class="navbar fixed-top navbar-expand-sm navbar-light">

    <img src="/navbar.png"/>

    <button class="btn" type="button" @onclick="e.Toggle">
        <span class="navbar-toggler-icon d-block d-sm-none"></span>
    </button>

    <!--Show if NavBar>sm  -->
    <div class="col-12 d-none d-sm-block" @onclick="e.Toggle">
        <NavBarLinks />
    </div>

    <!--Show if NavBar<=sm -->
    <div class="col-12 @e.ToggleTarget d-sm-none" @onclick="e.Toggle">
        <NavBarLinks />
    </div>

</nav>

